{
  "name": "ngx-walkthrough",
  "private": false,
  "version": "0.3.2",
  "description": "Build an Angular library compatible with AoT compilation and Tree shaking like an official package",
  "main": "./bundles/walkthrough.umd.js",
  "module": "./esm5/walkthrough.js",
  "es2015": "./esm2015/walkthrough.js",
  "scripts": {
    "build": "ng-packagr -p ng-packagr.json ",
    "test": "karma start",
    "test:watch": "karma start karma.conf.js --single-run false",
    "pack:lib": "ng-packagr -p ng-packagr.json && npm pack ./dist",
    "publish:lib": "ng-packagr -p ng-packagr.json && npm publish ./dist",
    "publish:lib:next": "ng-packagr -p ng-packagr.json && npm publish --tag next ./dist"
  },
  "typings": "./walkthrough.d.ts",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Altaba77/ngx-walkthrough.git"
  },
  "bugs": {
    "url": "https://github.com/Altaba77/ngx-walkthrough/issues"
  },
  "homepage": "https://github.com/Altaba77/ngx-walkthrough",
  "keywords": [
    "angular",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "tslib": "^1.7.1"
  },
  "peerDependencies": {
    "@angular/common": ">= 6.0.0",
    "@angular/core": ">= 6.0.0"
  },
  "devDependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/platform-server": "6.0.0",
    "@types/jasmine": "2.6.2",
    "@types/node": "8.0.47",
    "chalk": "2.3.0",
    "codelyzer": "4.0.2",
    "core-js": "2.5.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "karma": "6.3.14",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "3.0.5",
    "ng-packagr": "5.1.0",
    "reflect-metadata": "0.1.13",
    "rollup": "1.10.1",
    "rollup-plugin-license": "^0.13.0",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rxjs": "6.0.0",
    "shelljs": "0.7.8",
    "source-map-loader": "0.2.3",
    "ts-loader": "3.1.1",
    "tslint": "5.8.0",
    "tslint-angular": "1.0.0",
    "typescript": "2.7.2",
    "uglify-js": "3.1.6",
    "uglifyjs": "^2.4.11",
    "webpack": "3.8.1",
    "zone.js": "0.8.26"
  }
}
